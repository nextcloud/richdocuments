{
	"compilerOptions": {
		"outDir": "./dist/",
		"sourceMap": false,
		"noImplicitAny": false,
		"moduleResolution": "node",
		"module": "es6",
		"target": "es5",
		"jsx": "react",
		"allowJs": true
	},
	"include": ["src/**/*.ts", "src/**/*.tsx"],
	"exclude": [
		"node_modules",
		"vendor"
	],
	"typeRoots": ["node_modules/@types", "src/types"]
}
